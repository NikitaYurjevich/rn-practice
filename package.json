{
  "name": "dacon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "tsc": "tsc -p .",
    "tsw": "npm run tsc --watch",
    "tslint": "tslint -p .",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "release": "(cd android && ./gradlew app:assembleRelease) && npm run renameAndroid",
    "all": "npm install && (cd ios && pod install)",
    "podinstall": "(cd ios && arch -x86_64 pod install)",
    "reinstall": "rm -rf ./node_modules && npm install",
    "renameAndroid": "chmod u=rwx ./renameReleaseAndroidApk.bash && ./renameReleaseAndroidApk.bash",
    "clean": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.5",
    "@react-native-community/cli-platform-android": "7.0.1",
    "@react-native-community/progress-bar-android": "1.0.3",
    "@react-native-community/progress-view": "1.1.0",
    "@react-native-community/toolbar-android": "0.2.1",
    "@react-native-community/viewpager": "4.1.4",
    "axios": "0.21.4",
    "convertapi-js": "1.0.6",
    "jetifier": "1.6.6",
    "js-base64": "3.5.2",
    "lodash": "4.17.19",
    "metro-config": "0.70.1",
    "moment": "2.27.0",
    "odata": "1.1.0",
    "react": "16.13.1",
    "react-native": "0.61.5",
    "react-native-eject": "0.1.2",
    "react-native-gesture-handler": "1.7.0",
    "react-native-iphone-x-helper": "1.2.1",
    "react-native-keyboard-aware-scroll-view": "0.9.2",
    "react-native-svg": "12.1.0",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.2",
    "typescript-fsa-redux-thunk": "2.1.9"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "3.1.0",
    "@types/lodash": "4.14.159",
    "@types/react": "16.9.46",
    "@types/react-native": "0.63.7",
    "@types/react-native-calendars": "1.20.10",
    "@types/react-native-scrollable-tab-view": "0.10.1",
    "@types/react-redux": "7.1.9",
    "@types/redux-logger": "3.0.8",
    "babel-jest": "26.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.3.0",
    "metro-react-native-babel-preset": "0.62.0",
    "prettier": "2.7.1",
    "react-test-renderer": "16.13.1",
    "redux-logger": "3.0.6",
    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "react-native"
  }
}
